	<aside class="bg-dark">
	    <div class="container text-center">
	        <div class="call-to-action">
            <ul>
               <li class="style_image"><a href="https://twitter.com/AirtimeMG"><%= image_tag("tw.png", size: "45") %></a></li>
               <li class="style_image"><a href="https://www.youtube.com/channel/UCTVhAB0o7Ituz3sAipKj_DQ"><%= image_tag("you.png", size: "45") %></a></li>
                <li class="style_image"><a href="https://www.facebook.com/AirtimeMediaGroup"><%= image_tag("fb.png", size: "45") %></a></li>
            </ul>
	        </div>
	    </div>
	    <h6 style="text-align: center;">Copyright &copy; Airtime Media Group 2015</h6>
	</aside>
