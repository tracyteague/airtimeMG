<div class="jumbotron">
   <div class="container">
      <h1>Airtime Media Group</h1>
      <div id='break-bar'></div>
  </div>
</div>
<!-- Description -->

<div class="container">
        <h6 class="description-text">Airtime Media Group is a YouTube network founded on principals that revolve around the content creators.  Our main focus is to help our partners gain exposure and viewership along with business and marketing deals.</h6>
</div>

<!-- Team Section -->



    <!-- Team Section -->
    <section id="team" class="bg-light-gray">
        <div class="container">
                <div class="col-lg-12 text-center">
                    <h2 class="section-heading">Airtime Media Group Management</h2>
                </div>
                <div class="col-lg-4 col-sm-12">
                    <div class="team-member">
                    <%= image_tag("placeholder.png", class: "img-responsive img-circle") %>
                        <h4>Nick</h4>
                        <p class="text-muted">Co-Founder, Chief Financial Officer, Vice President</p>
                    </div>
                </div>

                <div class="col-lg-4 col-sm-12">
                    <div class="team-member">
                        <%= image_tag("mike.png", class: "img-responsive img-circle") %>
                        <h4>Mike</h4>
                        <p class="text-muted">Co-Founder, Chief Executive Officer, President and Chairman</p>
                    </div>
                </div>

                <div class="col-lg-4 col-sm-12">
                    <div class="team-member">
                      <%= image_tag("alex.png", class: "img-responsive img-circle") %>
                        <h4>Alex</h4>
                        <p class="text-muted">Co-Founder, Chief Operating Officer, Senior Vice President, Secretary and Director</p>
                    </div>
                </div>
            </div>
        </div>
        <a class="btn btn-lg btn-success fp-buttons" id="btn-talents" href="/ourtalents">
            Our Talents
        </a>
    </section>

 <!-- Core JavaScript -->
<script src="http://code.jquery.com/jquery-1.11.0.min.js"></script>
<script src="//maxcdn.bootstrapcdn.com/bootstrap/3.2.0/js/bootstrap.min.js"></script>
</body>